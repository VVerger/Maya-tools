{
    "load_package_once": true,
    "env": [
        {
            "ESMA_PLUGINS": "//Storage01/Partages/install/houdini/esma_plugins"
        },
        {
            "HOUDINI_VERS" :
            [
                {"houdini_version<'20.5.999' and houdini_version>='20.5'": "20.5.278"},
                {"houdini_version<'20.0' and houdini_version>='19.5.640'": "19.5.640"},
                {"houdini_version<'19.5.640' and houdini_version>='19.5.605'": "19.5.605"},
                {"houdini_version<'19.5.605' and houdini_version>='19.5.569'": "19.5.569"}
            ]
        },
        {
            "PY_VERS":
            [
                { "houdini_python=='python3.9'": "3.9" },
                { "houdini_python=='python3.10'": "3.10" },
                { "houdini_python=='python3.11'": "3.11" },
            ]
        },
        { "HOUDINI_PATH": "${ESMA_PLUGINS}/esma_tractor_spool/${PY_VERS}/${HOUDINI_VERS}" },
        { "HOUDINI_PATH": "${ESMA_PLUGINS}" }
    ]
}